{
	"name": "@ylide/ipfs-server",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"version": "1.0.0",
	"description": "Ylide Protocol IPFS Gateway",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/ylide-io/ipfs-server.git"
	},
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"build": "rimraf ./lib && tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint \"src/**/*.ts\" --fix"
	},
	"keywords": [
		"ylide",
		"messaging",
		"communication",
		"blockchain",
		"everscale",
		"ethereum"
	],
	"author": "Danila Simonov",
	"license": "ISC",
	"devDependencies": {
		"@types/busboy": "1.5.0",
		"@types/express": "4.17.17",
		"@types/node": "18.15.5",
		"@typescript-eslint/eslint-plugin": "5.56.0",
		"@typescript-eslint/parser": "5.56.0",
		"eslint": "8.36.0",
		"eslint-config-prettier": "8.7.0",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"prettier": "2.8.6",
		"rimraf": "4.4.0",
		"ts-node": "10.9.1",
		"typescript": "4.9.5"
	},
	"dependencies": {
		"@types/node-fetch": "2.6.2",
		"busboy": "1.6.0",
		"dotenv": "16.0.3",
		"express": "4.18.2",
		"form-data": "4.0.0",
		"node-fetch": "2.6.9",
		"stream-to-array": "^2.3.0"
	}
}
